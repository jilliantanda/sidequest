<%- include('./partials/head.ejs') %>

    <body id="quest">
        <h1 class="center-align">Quest log</h1>
        <div id="questcontainer">
            <h2>
                <%= quest.title %>
            </h2>
            <p>
                <%= quest.description %>
            </p>
        </div>

        <div class="imagediv">
            <%= quest.img %>
        </div>
        <div class="home"> <a class="waves-effect waves-light btn" id="button" href="/questlog/">Home</a></div>
        <a class="waves-effect waves-light btn" id="button" href="/questlog/<%=quest._id %>/edit">Edit</a>
        <form action="/fruits/<%=quest._id %>?_method=DELETE" method="POST">
            <input type="submit" value="DELETE">
            </form> 
    </body>